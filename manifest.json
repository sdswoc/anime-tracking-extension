{
    "name": "AniTrack",
    "version": "1.0",
    "description": "Update AniList when episode is completed",
    "permissions": [
        "storage",
        "identity",
        "https://graphql.anilist.co/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'sha256-AYhynCyON9tIWB6o3CsbGccG/vaLtJ3zsY91Elv2S/c=';object-src 'self'"
}