{
    "name": "AniTrack",
    "version": "1.0",
    "description": "Update AniList when episode is completed",
    "key": "",
    "permissions": [
        "storage",
        "identity",
        "https://graphql.anilist.co/*",
        "tabs",
        "declarativeContent"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://twist.moe/*"
            ],
            "js" : ["content.js"]
        },
        {
            "matches": [
                "https://kissanime.ru/*"
            ],
            "js" : ["contentKA.js"]
        }
    ],
    "background": {
        "scripts": ["background.js","auth.js","default.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}